<% content_for :head do %>
  <%= stylesheet_link_tag 'pages' %>
  <%= javascript_include_tag 'pages' %>
<% end %>

<%= link_to(image_tag(@page_url), @next_page_url ? @next_page_url : "#", :id => 'next_page_link') %>
<%= link_to("Previous page", @previous_page_url, :id => 'previous_page_link') if @previous_page_url %>