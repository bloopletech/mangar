page.replace_html("books", "") if params[:page] == '1'
page.insert_html(:bottom, "books", :partial => 'book', :collection => @books)
page << "update_books()"
puts "@books.total_pages: #{@books.total_pages}, params[:page]: #{params[:page]}"
if @books.total_pages == params[:page].to_i
  page << "last_page = true;"
end